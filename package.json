    "globby": "^6.1.0",
    "minimatch": "^3.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-plugin-transform-inline-imports-commonjs": "^1.2.0",
    "babel-preset-env": "^1.6.0",
    "classnames": "^2.2.5",
    "del-cli": "^1.1.0",
    "eslint": "^4.6.1",
